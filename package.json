{
  "name": "darylresume",
  "version": "0.1.22",
  "description": "resume",
  "author": "Daryl <daryl@blpc.us>",
  "repository": "git@github.com:darylwalsh/darylresume.git",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "NODE_ENV=production vue-cli-service build",
    "build:prefix": "BASE_URL=/resume/ yarn build",
    "lint": "NODE_ENV=production vue-cli-service lint './src/**/*.{vue,js}' '*.js' && prettier --write -l './src/**/*.{sass,css}' '*.json'",
    "deploy:gh": "yarn build:prefix && gh-pages -d dist/",
    "commit": "git-cz",
    "styleguide:build": "...",
    "prettier:check": "prettier --check './src/**/*.{vue,js}'",
    "validate": "run-s test lint prettier:check",
    "prerelease": "git-authors-cli && git add .",
    "release": "standard-version -a",
    "postrelease": "run-s release:*",
    "release:tags": "git push --follow-tags origin master",
    "release:github": "conventional-github-releaser -p angular",
    "ci:validate": "rm -rf node_modules && npm ci && npm run validate",
    "prepublishOnly": "npm run ci:validate && npm run build"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,css,scss}": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "vue": "^2.5.22"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.0.3",
    "conventional-changelog-cli": "^2.0.23",
    "conventional-github-releaser": "^3.1.3",
    "cz-adapter-eslint": "^0.3.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.1.0",
    "eslint-utils": "^1.4.2",
    "gh-pages": "^2.0.1",
    "git-authors-cli": "^1.0.18",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.11.0",
    "now": "^16.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "randomstring": "^1.1.5",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^7.0.0",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.0.0"
  },
  "contributors": [
    "Daryl <daryl@blpc.us>"
  ]
}
